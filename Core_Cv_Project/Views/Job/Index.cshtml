@using EntityLayer.Concrete
@model List<Job>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_JobDetailsLayout.cshtml";
}
<div class="contact-us section" id="contact">
    <div class="container">
        <div class="row">
            <div class="col-lg-6  align-self-center">
                <div class="section-heading">
                    @foreach (var item in Model)
                    {


                        <h1>@item.Title</h1>
                        <h4>@item.Content</h4>
                      
                    }
                </div>
              
            </div>
            @await Html.PartialAsync("FileUpload")
        </div>

    </div>
</div>

